{
  "name": "Juli",
  "description": "AppView template for WebCatalog",
  "version": "3.0.0",
  "main": "public/electron.js",
  "scripts": {
    "postinstall": "npx electron-builder install-app-deps",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint ./dist.js ./public ./src --ext js",
    "start": "react-scripts start",
    "electron-dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "dist": "yarn build && node dist.js",
    "test": "mocha ./tests"
  },
  "repository": "https://github.com/webcatalog/webcatalog-13",
  "author": "Quang Lam <quang.lam2807@gmail.com>",
  "dependencies": {
    "electron": "3.0.8",
    "electron-is-dev": "1.0.1",
    "electron-settings": "3.2.0",
    "electron-spellchecker": "1.1.2",
    "electron-window-state": "5.0.2",
    "request": "2.88.0",
    "request-promise-native": "1.0.5",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@material-ui/core": "3.4.0",
    "@material-ui/icons": "3.0.1",
    "algoliasearch": "3.30.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "classnames": "2.2.6",
    "concurrently": "4.0.1",
    "cross-env": "5.2.0",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "lodash.camelcase": "4.3.0",
    "prop-types": "15.6.2",
    "react": "16.6.1",
    "react-dom": "16.6.1",
    "react-redux": "5.1.0",
    "react-scripts": "2.1.1",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "semver": "5.6.0",
    "typeface-roboto": "0.0.54",
    "wait-on": "3.2.0"
  },
  "homepage": "./",
  "private": true,
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
